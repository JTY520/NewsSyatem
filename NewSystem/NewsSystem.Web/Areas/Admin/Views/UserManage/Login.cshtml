            <div class="login-html">
                <input type="radio" style="background-image:url(/Images/sign-bg.jpg)" class="sign-in" checked><label class="tab">Login</label>
                <div class="login-form">
                    <div class="group">
                        <label for="user" class="label">Username</label>
                        <input id="user" name="UserAccout" type="text" class="input">
                    </div>
                    <div class="group">
                        <label for="pass" class="label">Password</label>
                        <input id="pass" name="Password" type="password" class="input" data-type="password">
                    </div>
                    <div class="group">
                        <div class="button">
                            <div class="sign-btn"id="signUp"name="signUp" onclick="return Login();">登 陆</div>
                            <div name="onSignUp" id="onSignUp" style="width:0%;height:1px;background-color:#ffffff;float:left"></div>
                        </div>
                    </div>
                    <div class="hr"></div>
                    <div class="foot-lnk">
                        <label>Welcome To</label>
                    </div>
                </div>
                <script>
                </script>
            </div>
        </div>
